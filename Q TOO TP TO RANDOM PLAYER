wait("0.2")
-- LocalScript (This script should be placed in the player's character or StarterPlayerScripts)

local player = game.Players.LocalPlayer
local userInputService = game:GetService("UserInputService")
local players = game.Players:GetPlayers()

-- Function to teleport player to a random player
local function teleportToRandomPlayer()
    -- Remove the local player from the list to avoid teleporting to themselves
    local otherPlayers = {}
    for _, p in ipairs(players) do
        if p ~= player then
            table.insert(otherPlayers, p)
        end
    end

    -- Choose a random player from the remaining players
    if #otherPlayers > 0 then
        local randomPlayer = otherPlayers[math.random(1, #otherPlayers)]
        if randomPlayer.Character and randomPlayer.Character:FindFirstChild("HumanoidRootPart") then
            -- Teleport the player to the random player's position
            player.Character:SetPrimaryPartCFrame(randomPlayer.Character.HumanoidRootPart.CFrame)
        end
    end
end

-- Listen for the 'q' key press event
userInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end

    -- Check if the 'q' key was pressed
    if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Q then
        teleportToRandomPlayer()  -- Teleport to a random player
    end
end)
